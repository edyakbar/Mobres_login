<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agnis.mobres">

    <uses-permission android:name="android.permission.INTERNET" />
    <!--
             The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
             Google Maps Android API v2, but you must specify either coarse or fine
             location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/card"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UpdateProfile"
            android:label="@string/title_activity_update_profile"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UtamaActivity"
            android:label="@string/title_activity_utama"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListAgenActivity"
            android:label="@string/title_activity_list_agen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailAgenActivity"
            android:label="@string/title_activity_detail_agen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PetaActivity"
            android:label="@string/title_activity_peta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".KomentarActivity"
            android:label="@string/title_activity_komentar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProfilActivity"
            android:label="@string/title_activity_profil"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BantuanActivity"
            android:label="@string/title_activity_bantuan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FeedbackActivity"
            android:label="@string/title_activity_feedback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProviderActivity"
            android:label="@string/title_activity_provider"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AgenKuota"
            android:label="@string/title_activity_agen_kuota"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".KuotaActivity"
            android:label="@string/title_activity_kuota"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>